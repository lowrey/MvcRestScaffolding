<!DOCTYPE html>
<html lang="en">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
        <title>@ViewBag.Title</title>
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap.css")" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/bootstrap-responsive.css")" />
        <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/site.css")" />
        @RenderSection("Head", required: false)
    </head>
    <body>
        @RenderBody()
    </body>
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-1.9.0.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/bootstrap.js")"></script>

    @RenderSection("Bottom", required: false)
</html>
