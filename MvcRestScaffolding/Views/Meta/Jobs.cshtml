@using MvcRestScaffolding.Models;
@model List<JobViewModel>

@{
    ViewBag.Title = "Jobs";
}
@section Bottom
{
}

<div class="navbar navbar-inverse navbar-fixed-top">
  <div class="navbar-inner">
    <div class="container">
      <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>
      <a class="brand" href="#">MvcRestScaffolding</a>
      <div class="nav-collapse collapse">
        <ul class="nav">
          <li class="active"><a href="#">Home</a></li>
        </ul>
      </div><!--/.nav-collapse -->
    </div>
  </div>
</div>

<div class="container">
  <div class="hero-unit">
      <table class="table table-striped table-bordered"> 
      <tbody><tr>
        <th align="left">Job ID</th>
        <th align="left">Callback URL</th>
        <th align="left">Status</th>
        <th align="left">Data</th>
      </tr>
      @foreach (JobViewModel job in Model)
      {
      <tr>
        <td>@job.Id</td>
        <td>@job.CallbackUrl</td>
        <td>@job.Status</td>
        <td>@job.Data</td>
      </tr>
      }
    </tbody></table>
  </div>

  <hr>

  <footer>
  </footer>

</div> <!-- /container -->

